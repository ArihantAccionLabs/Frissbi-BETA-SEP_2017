<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_meeting_calendar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.frissbi.activities.MeetingCalendarActivity">

    <include
        android:id="@+id/calendar_view_layout"
        layout="@layout/calendar_view_layout" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/meeting_calendar_recyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/calendar_view_layout" />

</RelativeLayout>
